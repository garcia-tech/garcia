<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
	</PropertyGroup>
	<PropertyGroup>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="MMLib.SwaggerForOcelot" Version="7.0.1" />
		<PackageReference Include="Ocelot" Version="20.0.0" />
		<PackageReference Include="Ocelot.Cache.CacheManager" Version="20.0.0" />
		<PackageReference Include="Nerdbank.GitVersioning" Version="3.6.133">
			<PrivateAssets>all</PrivateAssets>
			<IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Ocelot.Provider.Consul" Version="20.0.0" />
		<PackageReference Include="Ocelot.Provider.Eureka" Version="20.0.0" />
	</ItemGroup>

</Project>
